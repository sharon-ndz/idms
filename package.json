{
  "name": "license-backend-api",
  "version": "0.0.2",
  "description": "License System Backend API",
  "author": "Victor Olubunmi",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "watch": "nest start --watch",
    "start:prod": "node dist/main",
    "start:debug": "nest start --debug --watch",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js",
    "migration:generate": "yarn typeorm -d ./src/config/typeorm.config.ts migration:generate",
    "migration:create": "yarn typeorm migration:create",
    "migration:run": "yarn typeorm -d ./src/config/typeorm.config.ts migration:run",
    "migration:revert": "yarn typeorm -d ./src/config/typeorm.config.ts migration:revert",
    "migration:show": "yarn typeorm -d ./src/config/typeorm.config.ts migration:show",
    "migration:drop": "yarn typeorm -d ./src/config/typeorm.config.ts schema:drop",
    "migration:reset": "yarn migration:drop && yarn migration:sync",
    "migration:sync": "yarn typeorm -d ./src/config/typeorm.config.ts schema:sync",
    "check:cycles": "madge --circular --extensions ts ./src",
    "seeder": "ts-node ./src/database/seed.ts",
    "seeder:create": "ts-node generators/seeder-g.ts",
    "app:install": "npm run db:drop && npm run db:create && npm run migrate && npm run seeder",
    "app:install:light": "npm run migration:drop && npm run migration:run && npm run seeder",
    "execute": "ts-node ./src/console.ts",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --verbose --detectOpenHandles --forceExit",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.723.0",
    "@aws-sdk/client-sqs": "^3.723.0",
    "@aws-sdk/s3-request-presigner": "^3.723.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "^2.0.6",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.1.13",
    "@nestjs/throttler": "^6.3.0",
    "@nestjs/typeorm": "^10.0.2",
    "@types/aws-lambda": "^8.10.140",
    "@types/mime": "^3.0.4",
    "@vendia/serverless-express": "^4.12.6",
    "axios": "^1.8.2",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "crypto-js": "^4.2.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "fs-extra": "^11.2.0",
    "helmet": "^8.0.0",
    "html-to-text": "^9.0.5",
    "mime": "3.0.0",
    "nodemailer": "^6.9.16",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.13.1",
    "qrcode": "^1.5.4",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.20",
    "typeorm-extension": "^3.6.3",
    "uuid": "^11.0.3",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.3.0",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@swc/cli": "^0.5.2",
    "@swc/core": "^1.10.4",
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^5.0.0",
    "@types/html-to-text": "^9.0.4",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/nodemailer": "^6.4.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "madge": "^8.0.0",
    "prettier": "^3.0.0",
    "serverless-dotenv-plugin": "^6.0.0",
    "serverless-jetpack": "^0.11.2",
    "serverless-offline": "^13.3.3",
    "serverless-plugin-typescript": "^2.1.5",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
